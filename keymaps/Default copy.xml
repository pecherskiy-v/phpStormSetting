<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="EditorDuplicateLines">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="MethodDown">
    <mouse-shortcut keystroke="alt button4" />
  </action>
  <action id="MethodUp">
    <mouse-shortcut keystroke="alt button5" />
  </action>
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="alt down" />
    <mouse-shortcut keystroke="alt button142" />
  </action>
  <action id="MoveLineUp">
    <mouse-shortcut keystroke="alt button143" />
    <keyboard-shortcut first-keystroke="alt up" />
  </action>
  <action id="QuickChangeScheme">
    <mouse-shortcut keystroke="control button5" />
  </action>
</keymap>